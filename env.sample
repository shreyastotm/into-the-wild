# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Supabase OAuth Credentials (set in Supabase Dashboard, not .env)
# These are configured in Supabase Dashboard > Authentication > Providers
# SUPABASE_AUTH_EXTERNAL_GOOGLE_CLIENT_ID=your_google_client_id
# SUPABASE_AUTH_EXTERNAL_GOOGLE_SECRET=your_google_client_secret
# SUPABASE_AUTH_EXTERNAL_FACEBOOK_CLIENT_ID=your_facebook_app_id
# SUPABASE_AUTH_EXTERNAL_FACEBOOK_SECRET=your_facebook_app_secret

# Application Configuration
VITE_APP_NAME="Into The Wild"
VITE_APP_VERSION="1.0.0"
VITE_APP_ENV=development

# Security Configuration
VITE_MAX_FILE_SIZE=2097152
VITE_ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/webp

# Rate Limiting (client-side)
VITE_RATE_LIMIT_REQUESTS=10
VITE_RATE_LIMIT_WINDOW=60000

# Feature Flags
VITE_ENABLE_GOOGLE_AUTH=true
VITE_ENABLE_INSTAGRAM_AUTH=true
VITE_ENABLE_FILE_UPLOADS=true
VITE_ENABLE_ANALYTICS=false

# Google Analytics 4 (GA4) Configuration
# Get your Measurement ID from Google Analytics Dashboard
# Format: G-XXXXXXXXXX
# Your Measurement ID: G-NW4MTHFT60
VITE_GA4_MEASUREMENT_ID=G-NW4MTHFT60

# Backend API
VITE_BACKEND_URL=http://localhost:3003